// tentativas
([1-2][0-5]{2}|[1-9]{1,2}|[0-9])(\.[1-2][0-5]{2}|[1-9]{1,2}|[0-9])
(\.[1-2][0-5]{2}|[0-9]|[1-9]{2}){3}
(\.[1-2][0-5]{2}|[0-9]|[1-9]{2})+

(\.[0-9]{1}|[1-9][0-9]|[1-2][0-5]{2}){3}

([1-2][0-5]{1,3}|[0-9])(\.([1-2][0-5]{1,3}|[0-9])){1,3}"


(?:1[0-9]{1,2})|(?:2[0-5]{1,2})|(?:[1-9][0-9])|(?:[0-9])

(2[0-5]{2})|(1[0-9]{2})|([1-9][0-9])|([0-9])(\.((2[0-5]{2})|(1[0-9]{2})|([1-9][0-9])|([0-9]))){3}

2[0-5]{2}|1[0-9]{2}|[1-9][0-9]|[0-9](\.(2[0-5]{2}|1[0-9]{2}|[1-9][0-9]|[0-9])){3}